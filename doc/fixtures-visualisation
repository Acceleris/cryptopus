@startuml

title Fixtures


package "team" {
object team1
object team2


team1 : description = root, public
team1 : visible = true
team1 : private = false
team1 : noroot = false

team2 : description = root, public
team2 : visible = true
team2 : private = true
team2 : noroot = true

}


package "group" {
object Group1
object Group2

Group1 *-- team1
Group2 *-- team2

}

package "account" {
object account1
object account2

account1 *-- Group1
account2 *-- Group2

account1 : accountname = test
account1 : password = password

account2 : accountname = test2
account2 : password = password
}


package "item" {
object item1

item1 *-- account1

item1 : filename= item1
item1 : content_type = text

}

package "teammember" {
object team1_bob
object team2_bob
object team1_root
object team1_alice
object team1_admin

}

package "user" {
object user_root
object user_bob
object user_alice
object user_admin


user_root : username = root
user_root : password = password

user_bob : username = bob
user_bob : password = password

user_alice : username = alice
user_alice : password = password

user_admin : username = admin
user_admin : password = password




team1_bob *-- user_bob
team1_bob *-up- team1
team2_bob *--- user_bob
team2_bob *-up- team2

team1_root *--- user_root
team1_root *-up- team1

team1_alice *--- user_alice
team1_alice *-up- team1

team1_admin *--- user_admin
team1_admin *-up- team1

}


